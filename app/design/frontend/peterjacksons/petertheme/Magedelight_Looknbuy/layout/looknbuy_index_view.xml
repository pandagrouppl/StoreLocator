<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        
        <script src="prototype/prototype.js"/>
        <script src="jquery.js"/>
                <link src="Magedelight_Looknbuy::js/looknbuy.js"/>
                  <link src="Magedelight_Looknbuy::js/jquery.noconflict.js"/>
        
    </head>

    <update handle="page_calendar"/>
    <body>
        <referenceContainer name="columns.top">
            <block class="PandaGroup\LooknbuyExtender\Block\Title" name="page.main.title" template="Magedelight_Looknbuy::title.phtml"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Magedelight\Looknbuy\Block\View" name="view.container" template="Magedelight_Looknbuy::view.phtml" cacheable="false">
                <block class="Magedelight\Looknbuy\Block\View" name="social.facebook" as="facebook" template="Magedelight_Looknbuy::social/facebook.phtml" cacheable="false" />
                <block class="Magedelight\Looknbuy\Block\View" name="social.twitter" as="twitter" template="Magedelight_Looknbuy::social/twitter.phtml" cacheable="false" />
                <block class="Magedelight\Looknbuy\Block\View" name="social.pinit" as="pinit" template="Magedelight_Looknbuy::social/pinit.phtml" cacheable="false" />
                <block class="PandaGroup\LooknbuyExtender\Block\Title" name="looknbuy.links" template="Magedelight_Looknbuy::title.phtml"/>
            </block>

            <referenceBlock name="looknbuy.product.addto">
                <block class="Magento\Wishlist\Block\Catalog\Product\ProductList\Item\AddTo\Wishlist"
                       name="looknbuy.product.addto.wishlist" as="wishlist" before="compare"
                       template="Magento_Wishlist::catalog/product/list/addto/wishlist.phtml"/>
            </referenceBlock>
        </referenceContainer>
    </body>

 
</page>