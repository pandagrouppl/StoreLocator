<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <script src="prototype/prototype.js"/>
    </head>
    <body>
        <referenceContainer name="columns.top">
            <block class="Magento\Theme\Block\Html\Title" name="page.main.title" template="Magedelight_Looknbuy::looks/title.phtml"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Magedelight\Looknbuy\Block\Looks" name="discount.container" template="Magedelight_Looknbuy::looks.phtml" cacheable="false">
            </block>
            <!--<referenceBlock name="upsell.product.addto.compare" remove="true" />-->
        </referenceContainer>
    </body>
</page>
